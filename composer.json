{
	"name": "anlutro/l4-testing",
	"description": "Base PHPUnit classes for Laravel 4.",
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"laravel/framework": "~5.0",
		"symfony/browser-kit": "~2.5"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"laravel/laravel": "dev-develop"
	},
	"autoload": {
		"psr-4": {
			"anlutro\\LaravelTesting\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-develop": "0.3.x-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
